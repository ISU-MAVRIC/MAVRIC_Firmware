#include "TimerPWM_Servo.hpp"

InternalServoControl::InternalServoControl(Peripherials::Timer& timer, Peripherials::TimerCapComUnit unit) : angle(0), _timer(timer), module(unit)
{
	// 20 ms
	_timer.SetPeriod(0.020f);


}
